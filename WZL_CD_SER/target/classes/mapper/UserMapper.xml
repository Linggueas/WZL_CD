<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.wzl_cd_ser.mapper.UserMapper">
    <insert id="insertUser" parameterType="com.example.wzl_cd_ser.pojo.User">
        insert into user(email,password,online)
        values (#{email},#{password},#{online});
    </insert>
    <update id="updateUseronline" parameterType="com.example.wzl_cd_ser.pojo.User">
        update user
        set online = 1
        where email = #{email}
    </update>
</mapper>